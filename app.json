{
  "expo": {
    "name": "Echo",
    "slug": "echo",
    "version": "1.0.0",
    "scheme": "echo",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.echo.app"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.echoreminders.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "pantheon-industries",
          "project": "echo"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "596a7811-5d10-4819-83d6-fa2def136f77"
      }
    }
  }
}
